{
  "name": "@tyconsa/bizuit-ui-components",
  "version": "1.0.0",
  "description": "Production-ready, fully customizable UI components for Bizuit BPM forms - Built with Radix UI, Tailwind CSS, and React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bizuit",
    "bpm",
    "forms",
    "ui",
    "components",
    "react",
    "typescript",
    "radix-ui",
    "tailwind",
    "shadcn",
    "datagrid",
    "combobox",
    "datepicker",
    "responsive",
    "mobile"
  ],
  "author": "Bizuit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bizuit/form-template.git",
    "directory": "packages/bizuit-ui-components"
  },
  "homepage": "https://github.com/bizuit/form-template#readme",
  "bugs": {
    "url": "https://github.com/bizuit/form-template/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tyconsa/bizuit-form-sdk": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-table": "^8.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "date-fns": "^3.0.0",
    "lucide-react": "^0.302.0",
    "react-day-picker": "^8.10.0",
    "react-hook-form": "^7.49.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@tyconsa/bizuit-form-sdk": "file:../bizuit-form-sdk",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "postcss": "^8.4.32",
    "storybook": "^7.6.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
