# Production Environment Variables for Runtime App
# Copy this file as .env.local on the server after deployment

# Base path for IIS deployment
NEXT_PUBLIC_BASE_PATH=/arielschBIZUITCustomForms

# Backend API URL (relative path, IIS will resolve to test.bizuit.com/arielschBIZUITCustomFormsbackend)
NEXT_PUBLIC_BIZUIT_FORMS_API_URL=/arielschBIZUITCustomFormsbackend
NEXT_PUBLIC_BIZUIT_DASHBOARD_API_URL=/arielschBIZUITCustomFormsbackend

# Timeouts and configuration
NEXT_PUBLIC_BIZUIT_TIMEOUT=30000
NEXT_PUBLIC_BIZUIT_TOKEN_EXPIRATION_MINUTES=1440

# Environment
NODE_ENV=production
